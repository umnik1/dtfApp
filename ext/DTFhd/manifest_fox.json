{
    "name": "DTF HD",
    "version": "0.1",
    "permissions": [
      "https://dtf.ru/*"
    ],
    "content_scripts": [
      {
        "matches": ["https://dtf.ru/*"],
        "js": ["jquery-3.6.0.min.js", "FileSaver.min.js", "hd.js"],
        "css": ["hd.css"],
        "run_at": "document_end"
      }
    ],
    "icons": { 
      "48": "icons/icon48.png",
      "96": "icons/icon96.png"
    },
    "manifest_version": 2,
    "browser_specific_settings": {
      "gecko": {
        "id": "umnikone@yandex.ru",
        "strict_min_version": "42.0"
      }
    }
  }